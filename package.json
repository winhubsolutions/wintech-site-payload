    "react-dom": "^19.2.0",
    "react-fast-marquee": "^1.6.5",
    "react-hook-form": "^7.65.0",
    "react-icons": "^5.5.0",
    "sharp": "0.34.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",

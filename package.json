    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@styleglide/kit-view-provider": "^0.0.8",
    "@types/mdx": "^2.0.13",
    "@vercel/blob": "^2.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "embla-carousel-react": "^8.6.0",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
